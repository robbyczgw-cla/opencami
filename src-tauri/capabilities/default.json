{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables desktop, plugin, and window permissions",
  "windows": [
    "main",
    "window-*"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-create",
    "notification:default",
    "autostart:default",
    "clipboard-manager:default"
  ]
}
